        sens.05 =c(sens.05, max(auc.plot.stage.1[auc.plot.stage.1$fpr < 0.05,'tpr']))
        sens.01 = c(sens.01,max(auc.plot.stage.1[auc.plot.stage.1$fpr < 0.01,'tpr']))
        
      }
